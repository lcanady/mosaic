{
  "name": "@mosaic/mu",
  "version": "0.0.1",
  "description": "A Modern MUX-Like Server Framework for Node.js",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "set NODE_ENV=dev && concurrently \"ts-node-dev --respawn --transpile-only src/app.ts\" \"ts-node-dev --respawn --transpile-only src/telnet.ts\"",
    "start": "set NODE_ENV=prod && ts-node src/processManager.ts"
  },
  "keywords": [
    "MUX",
    "MUSH",
    "MUD"
  ],

    "@types/node": "^20.12.10",
    "@types/promptly": "^3.0.5",
    "@types/socket.io": "^3.0.2",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@digibear/tags": "^0.4.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@ursamu/parser": "^1.3.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.6.1",
    "pm2": "^5.3.1",
    "prettier": "^3.2.5",
    "promptly": "^3.2.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  }
